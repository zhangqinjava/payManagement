<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.al.account.mapper.AccountMapper">
    <select id="selectForUpdate" resultType="com.al.account.bean.vo.AccountVo">
        select id,
               account_no,
               store_id,
               channel_code,
               channel_account_no,
               account_type,
               balance,
               frozen_balance,
               transit_balance,
               account_status,
               create_time,
               update_time
            from t_set_account
            <where>
                account_no IN
                <foreach collection="list"
                         item="no"
                         open="("
                         close=")"
                         separator=",">
                    #{no}
                </foreach>
            </where>




    </select>

</mapper>